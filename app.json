{
  "expo": {
    "name": "Dérives",
    "slug": "Derives",
    "owner": "derives",
    "description": "Dérives est une expérience poétique visuelle et sonore.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/images/*",
      "assets/sounds/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "fr.utc.derives"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
